{
  "compilerOptions": {
    "jsx": "react",
    "module": "es6",
    "noImplicitAny": true,
    "strict": true,
    "moduleResolution": "node",
    "outDir": "./dist/",
    "preserveConstEnums": true,
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "sourceMap": true,
    "downlevelIteration": true
  },
  "include": ["./src"]
}
